-- Operadores Relacionais --

 5 > 2 - true

 7 < 4 - false

 8 >= 8 - true

 9 <= 7 - false

 5 == 5 - true

 4 != 4 - false

 5 == 5 - true

 5 == '5' - true 

 5 === '5' - false


-- Operadores Lógicos -- 

! -> negação -> true or false

&& -> Conjunção -> true and true

|| -> disjunção  > um dos valores precisa ser true



-- Ordem de execução --


1º () ** / ...

2º > < >= ...

3º ! && || ...

-- Operadores Ternários --


? = if
: = else
